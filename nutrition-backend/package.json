		"dotenv": "^16.0.0",
		"eslint": "^9.17.0",
		"express": "^4.21.2",
		"express-fileupload": "^1.4.0",
		"express-rate-limit": "^6.4.0",
		"express-request-id": "^2.0.1",
		"express-validation": "^4.1.1",
		"flatted": "^3.3.1",
		"helmet": "^5.0.2",
		"http-errors": "^2.0.0",
		"joi": "^17.6.0",
