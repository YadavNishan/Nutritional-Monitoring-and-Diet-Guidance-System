{
	"name": "nutritional-monitoring-backend",
	"version": "1.0.0",
	"description": "Backend build in with Express, PostgreSQL, Swagger with RMVCS architecture",
	"main": "app.js",
	"scripts": {
		"test": "mocha './test/user-auth.test.js'",
		"dev": "nodemon ./app.ts",
		"start": "node ./dist/app.js",
		"migrate": "sequelize db:migrate --env=database",
		"migrate:undo": "sequelize db:migrate:undo --env=database",
		"migrate:undo:all": "sequelize db:migrate:undo:all --env=database",
		"generateMigration": "sequelize migration:create --name",
		"seed:all": "sequelize db:seed:all --env=database",
		"seed:undo:all": "sequelize db:seed:undo:all --env=database",
		"build": "npm run clean && yarn && npm run build-ts ",
		"clean": "rimraf ./dist",
		"build-ts": "tsc",
		"prettier": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint ./src/**/*.ts"
	},
	"keywords": [],
	"author": "Aaryash Shakya",
	"license": "ISC",
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-fileupload": "^1.2.2",
		"@types/http-errors": "^1.8.2",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/method-override": "0.0.32",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.25",
		"@types/nodemailer": "^6.4.15",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^8.3.4",
		"chai": "^5.1.1",
		"mocha": "^10.6.0",
		"nodemon": "^2.0.15",
		"prettier": "^3.4.2",
		"supertest": "^7.0.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@handlebars/allow-prototype-access": "^1.0.5",
		"@pinecone-database/pinecone": "^4.0.0",
		"@types/express-request-id": "^1.4.3",
		"@types/node-cron": "^3.0.11",
		"@types/node-fetch": "^2.6.2",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
